	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"proxy": "http://localhost:3001",
	"devDependencies": {
		"concurrently": "^4.1.2",
		"cross-env": "^5.2.1",
		"nodemon": "^1.19.2"
	}
